package entregable1;
import entregable1.Fecha;


/**
 *
 * @author p.hernandez.lazaro
 */

public class Publicacion {
    private long fecha;
    private String texto;
    
    public Publicacion(long fecha, String texto){
        fecha=this.fecha;
        texto=this.texto;
    }
    
    public String toString(){
        String fechaString=Fecha.fechaNumericaString(fecha);
        return fechaString+"->"+texto;
        
    }
}
